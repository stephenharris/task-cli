    "inquirer": "^9.1.4",
    "moment": "^2.29.4",
    "octokit": "^2.0.10",
    "path": "^0.12.7",
    "uuid": "^9.0.0",
    "yaml": "^2.1.3",
    "yaml-schema": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
